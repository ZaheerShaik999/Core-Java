package com.dl.one;

public class Eg3 {
	
	//Relational Operators / Comparison Operators < > <= >= == !=
	public static void main(String[] args) {
		
		int a = 10;
		int b = 5;
		int c = 10;
		
		System.out.println(a == b); // false
		System.out.println(a == c); // true
		System.out.println(a < b); // false
		System.out.println(b < a); // true
		System.out.println(a <= c); // true
		System.out.println(a >= c); // true
		System.out.println(a != c); // false
		
	}
}
