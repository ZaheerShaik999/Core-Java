package com.dl.one;

public class Eg2 {

	public static void main(String[] args) {

		// Assignment Operators += -= *= /=  %=  ==
		int a = 10;
		int b = 20;
		int c = 10;
		
		System.out.println(a+=1); //a = a+ 1, a = 10+ 1, 11
		
		a+=1; // a = a + 1, a = 11 + 1, a = 12
		
		System.out.println(a);
		
		System.out.println(a += b); // a = a+b, a = 12+20, 32
		
	}
}
